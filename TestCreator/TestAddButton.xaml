<UserControl x:Class="TestCreator.TestAddButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TestCreator"
             mc:Ignorable="d" 
             d:DesignHeight="40" d:DesignWidth="40">
    <Button Command="{Binding Path=AddTest}">
        <Grid>
            <Line Stroke="DarkBlue" StrokeThickness="6"  X1="20" Y1="10" X2="20" Y2="30" Width="38" Height="38" Margin="-2,-2,0,0"/>
            <Line Stroke="DarkBlue" StrokeThickness="6"  Y1="20" X1="10" Y2="20" X2="30" Width="38" Height="38" Margin="-2,-2,0,0"/>
        </Grid>
    </Button>
</UserControl>
